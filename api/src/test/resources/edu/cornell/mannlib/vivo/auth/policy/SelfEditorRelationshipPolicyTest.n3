	a foaf:Person ;
	a owl:Thing ;
	a core:EmeritusProfessor ;
	rdfs:label "Person, Bozo" ;
	foaf:firstName "Bozo" ;
	foaf:lastName "Person" ;
	core:relatedBy mydomain:authorshipBozo ;
	core:relatedBy mydomain:editorshipBozo ;
	.

