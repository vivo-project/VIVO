PREFIX core: <http://vivoweb.org/ontology/core#>

CONSTRUCT {
     ?subjectArea ?property ?object .
} WHERE {
     PERSON_URI core:relatedBy ?issuedCredential .
     ?issuedCredential a core:IssuedCredential .
     ?issuedCredential core:hasSubjectArea ?subjectArea .
     ?subjectArea ?property ?object .
}
