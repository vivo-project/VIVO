<!-- VIVO-specific default list view config file for object properties

     See guidelines at https://wiki.duraspace.org/x/ExUdB  -->

<list-view-config>
    <query-select>

        SELECT ?value
        WHERE {

                ?subject ?property ?value
                FILTER isLiteral(?value)

        } ORDER BY ?value
    </query-select>

    <query-construct>

        CONSTRUCT {
            ?subject ?property ?value
        } WHERE {
                ?subject ?property ?value
        }
    </query-construct>

    <template>propStatement-scopusId.ftl</template>
</list-view-config>
